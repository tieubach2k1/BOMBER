<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<VBox xmlns:fx="http://javafx.com/fxml/1" alignment="TOP_CENTER" prefHeight="390.0" prefWidth="517.0"
      stylesheets="@style.css" xmlns="http://javafx.com/javafx/11.0.1"
      fx:controller="uet.oop.bomberman.menu.MenuPanelController">

   <ImageView fitHeight="335.0" fitWidth="517.0" pickOnBounds="true">
      <Image url="@../../../../../res/images.jpg"/>
   </ImageView>
   <Button id="btnStart" alignment="CENTER" layoutX="66.0" layoutY="200.0" mnemonicParsing="false"
           onAction="#openPlayPanel" prefHeight="147.0" prefWidth="517.0" stylesheets="@style.css" text="Start Game"
           textAlignment="CENTER" textFill="WHITE">
      <font>
         <Font name="System Bold" size="23.0"/>
      </font>
   </Button>
</VBox>
